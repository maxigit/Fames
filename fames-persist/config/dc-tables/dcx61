    remoteId Text  sql=remote_id
    message Text  sql=message
    messageVariables ByteString  sql=message_variables
    amount Int  sql=amount
    currencyCode Text  sql=currency_code
    status Text  sql=status
    remoteStatus Text  sql=remote_status
