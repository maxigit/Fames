
FieldRevisionFieldLinkT sql=dcx_field_revision_field_link
    entityType Text  sql=entity_type
    bundle Text  sql=bundle
    deleted Bool  sql=deleted
    entityId Int  sql=entity_id
    revisionId Int  sql=revision_id
    language Text  sql=language
    delta Int  sql=delta
    fieldLinkUrl Text Maybe sql=field_link_url default=NULL
    fieldLinkTitle Text Maybe sql=field_link_title default=NULL
    fieldLinkAttributes Text Maybe sql=field_link_attributes default=NULL
    Primary entityType deleted entityId revisionId language delta
